{
  "name": "@algoan/nestjs-pagination",
  "version": "5.0.3",
  "description": "NestJS interceptor handling request's pagination",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -p .",
    "test:cov": "nyc --reporter=html --reporter=lcov npm test",
    "pretest": "npm run build",
    "test": "NODE_ENV=test mocha --reporter spec --exit --require ts-node/register ./test/**/*.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/algoan/nestjs-components.git"
  },
  "files": [
    "dist/src"
  ],
  "keywords": [
    "nodejs",
    "typescript",
    "nestjs",
    "pagination",
    "interceptor"
  ],
  "author": "ccoeurderoy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/algoan/nestjs-components.git"
  },
  "homepage": "https://github.com/algoan/nestjs-components/packages/nestjs-pagination",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "cache": false,
    "extension": [
      ".ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "content-range": "^1.1.0",
    "format-link-header": "^3.1.1"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7",
    "@nestjs/core": ">=7",
    "@nestjs/platform-express": ">=7",
    "rxjs": ">=7",
    "express": ">=4"
  }
}
